{
  "name": "braican/headless-wordpress",
  "description": "A theme for a headless WordPress site that produces a REST API.",
  "version": "0.0.1",
  "type": "wordpress-theme",
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "composer/installers": "*",
    "wpackagist-plugin/advanced-custom-fields":"5.8.1"
  },
  "extra": {
    "installer-paths": {
      "../../plugins/{$name}/": [
        "type:wordpress-plugin"
      ]
    }
  }
}
